<!doctype html>
<html>
    <head>
        <title>La pesquiña</title>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
         <script src="javascripts/atari/jquery-1.7.2.min.js" ></script>
         <link rel="stylesheet" type="text/css" href="stylesheets/estilo.css">
    </head>
    <body class="lista">
<div class="puntos">
    <table>
        <tr>
            <th class="num">
                Lugar
            </th>
            <th class="col b">
                Nombre
            </th>
            <th class="col b">
                Email
            </th>
            <th class="col b">
                ¿Baneado?
            </th>
            <th class="col b">
                Puntos
            </th>
            <th class="col b">
                Trampas en puntuaje
            </th>
            <th class="col b">
                Fecha
            </th>
            <th class="col b">
                Imagen Usuario
            </th>
        </tr>
    </table>
    <div class="marcador mc">
    <table>
        <% for(var a=0;a<marcadores.length;a++){ if(marcadores[a] ) {%>

	
        <tr>
            <td class="num">
                <%=(a+1)%>
            <td>
            <td class="col b">
                <a href='http://facebook.com/<%=marcadores[a].id%>'>
                    <%= marcadores[a].nombre %>
                </a>
            </td>
            <td class="col b">
                <%= marcadores[a].email %>
            </td>
            <td class="col b">
                <%= marcadores[a].trampa %>
            </td>
            
            <td class="col b">
                <%= marcadores[a].puntos %>
            </td>
            <td class="col b">
                <%= marcadores[a].bans %>
            </td>
            <td class="col b">
                <%= marcadores[a].fecha %>
            </td>
            <td class="col b">
                <div class="img o">
                    <img src='uploads/<%= marcadores[a].id %>.png'>
                <div>
            </td>
        </tr>

		<% } %>
	<% } %>

    </table>
    </div>
</div>
